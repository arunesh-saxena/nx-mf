{
  "name": "orders",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "orders",
    "sourceRoot": "apps/orders/src",
    "targets": {
      "build": {
        "executor": "@nx/webpack:webpack",
        "outputs": [
          "{options.outputPath}"
        ],
        "defaultConfiguration": "production",
        "options": {
          "compiler": "babel",
          "outputPath":  "apps/orders/dist",
          "index":  "apps/orders/src/index.html",
          "baseHref": "/",
          "main":  "apps/orders/src/main.ts",
          "tsConfig":  "apps/orders/tsconfig.app.json",
          "assets": [
             "apps/orders/src/favicon.ico",
             "apps/orders/src/assets"
          ],
          "styles": [
             "apps/orders/src/styles.scss"
          ],
          "scripts": [],
          "webpackConfig":  "apps/orders/webpack.config.ts"
        },
        "configurations": {
          "development": {
            "extractLicenses": false,
            "optimization": false,
            "sourceMap": true,
            "vendorChunk": true
          },
          "production": {
            "fileReplacements": [
              {
                "replace":  "apps/orders/src/environments/environment.ts",
                "with":  "apps/orders/src/environments/environment.prod.ts"
              }
            ],
            "optimization": true,
            "outputHashing": "all",
            "sourceMap": false,
            "namedChunks": false,
            "extractLicenses": true,
            "vendorChunk": false
          }
        },
        "dependsOn": [
          "^build",
          "typecheck"
        ]
      },
      "serve": {
        "executor": "@nx/react:module-federation-dev-server",
        "defaultConfiguration": "development",
        "options": {
          "buildTarget": "orders:build",
          "hmr": true,
          "port": 4202
        },
        "configurations": {
          "development": {
            "buildTarget": "orders:build:development"
          },
          "production": {
            "buildTarget": "orders:build:production",
            "hmr": false
          }
        },
        "dependsOn": [
          "typecheck"
        ]
      },
      "serve-static": {
        "executor": "@nx/react:module-federation-static-server",
        "defaultConfiguration": "production",
        "options": {
          "serveTarget": "orders:serve"
        },
        "configurations": {
          "development": {
            "serveTarget": "orders:serve:development"
          },
          "production": {
            "serveTarget": "orders:serve:production"
          }
        }
      },
      "test": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig":  "apps/orders/jest.config.cts"
        }
      }
    }
  },
  "exports": {
    "./Module": {
      "@org/source": "./src/remote-entry.ts",
      "types": "./src/remote-entry.ts",
      "import": "./src/remote-entry.ts",
      "default": "./src/remote-entry.ts"
    }
  },
  "types": "./src/remote-entry.ts"
}
